<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.unb.cic.controller.ControllerMain">
   <children>
      <Button fx:id="btnEntrar" layoutX="222.0" layoutY="333.0" mnemonicParsing="false" onAction="#initLogin" text="Entrar" />
      <TextField fx:id="txtUsuario" layoutX="284.0" layoutY="228.0" />
      <Label fx:id="lblUsuario" layoutX="220.0" layoutY="232.0" text="Usuário" />
      <Label fx:id="lblSenha" layoutX="224.0" layoutY="270.0" text="Senha" />
      <PasswordField fx:id="txtSenha" layoutX="284.0" layoutY="266.0" />
      <Label alignment="CENTER" layoutX="246.0" layoutY="138.0" prefHeight="58.0" prefWidth="149.0" text="Sistema" textFill="#2600ae">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnSair" layoutX="303.0" layoutY="333.0" mnemonicParsing="false" onAction="#closeLogin" text="Sair" />
      <Button fx:id="btnCadastrarUsuario" layoutX="368.0" layoutY="333.0" mnemonicParsing="false" onAction="#cadastrarUsuario" text="Cadastrar usuário" />
      <Button fx:id="btnTabelas" layoutX="303.0" layoutY="383.0" mnemonicParsing="false" onAction="#criarTabela" text="Tabelas" />
   </children>
</AnchorPane>
